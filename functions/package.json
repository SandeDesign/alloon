{
  "name": "alloon-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for FLG-Administratie",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^5.0.0",
    "googleapis": "^140.0.0",
    "busboy": "^1.6.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.4",
    "@types/cors": "^2.8.17",
    "typescript": "^5.4.0",
    "firebase-functions-test": "^3.2.0"
  },
  "private": true
}
